{
  "name": "browser-home",
  "version": "1.0.0",
  "devDependencies": {
    "adm-zip": "^0.4.13",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "filereader": "^0.10.3",
    "image-size": "^0.8.3",
    "jsdom": "^15.2.0",
    "md5-file": "^4.0.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "precache-icons": "mkdir -p public/precached-icons && node src/scripts/update-blocklist.js ./blocklist.txt && node src/scripts/download-icons.js ./blocklist.txt public/precached-icons",
    "update-version": "node src/scripts/update-version.js refs.tags.v1.1.0-test2"
  }
}
