{
  "manifest_version": 2,
  "name": "Browser Home",
  "version": "0.0.0",
  "description": "Displays a page of your favorite bookmarks when opening a new tab.",
  "author": "RandomDevelopers",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "background.js"
    ]
  },
  "icons": {
    "64": "favicon.png"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "*://*/*"
  ],
  "options_ui": {
    "page": "index.html?options"
  }
}
